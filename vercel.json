{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "installCommand": "pnpm install --frozen-lockfile",
  "buildCommand": "pnpm run build",
  "devCommand": "pnpm run dev",
  "functions": {
    "app/api/**/*": {
      "runtime": "nodejs22.x"
    }
  },
  "env": {
    "SANDBOX_PROVIDER": "vercel"
  }
}
